{%- for family in instructions['kems'] %}
if(OQS_ENABLE_KEM_{{ family['name']|upper }})
    add_subdirectory(kem/{{ family['name'] }})
    list(APPEND OQS_KEM_OBJS {% raw -%}${{%- endraw-%}OQS_{{ family['name']|upper }}_OBJS})
endif(){% endfor %}
{%- for family in instructions['sigs'] %}
if(OQS_ENABLE_SIG_{{ family['name']|upper }})
    add_subdirectory(sig/{{ family['name'] }})
    list(APPEND OQS_SIG_OBJS {% raw -%}${{%- endraw-%}OQS_{{ family['name']|upper }}_OBJS})
endif(){% endfor %}

